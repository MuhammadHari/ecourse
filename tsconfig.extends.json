      "@models/*": ["views/models/*"],
      "@model": ["views/models"],
      "@teacher-pages/*": ["views/pages/teacher/*"],
      "@teacher-page": ["views/pages/teacher"],
      "@student-pages/*": ["views/pages/student/*"],
      "@student-page": ["views/pages/studen"],
      "@guest-pages/*": ["views/pages/guest/*"],
      "@guest-page": ["views/pages/guest"]
    }
  }
}
