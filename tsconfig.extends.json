{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": ["views/*"],
      "@components/*": ["views/components/*"],
      "@fields/*": ["views/components/form-fields/*"],
      "@field": ["views/components/form-fields"],
      "@hooks/*": ["views/hooks/*"],
      "@providers/*": ["views/providers/*"],
      "@pages/*": ["views/pages/*"],
      "@utils/*": ["views/utils/*"],
      "@services/*": ["views/services/*"],
